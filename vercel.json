{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "https://your-backend.onrender.com/api/v1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://your-backend.onrender.com",
    "NEXT_PUBLIC_WS_URL": "wss://your-backend.onrender.com"
  }
}